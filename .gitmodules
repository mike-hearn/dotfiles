[submodule "base16/.config/base16-shell"]
	path = base16/.config/base16-shell
	url = https://github.com/chriskempson/base16-shell.git
[submodule "tmux/.tmux/plugins/tpm"]
	path = tmux/.tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
[submodule "zsh/.oh-my-zsh"]
	path = zsh/.oh-my-zsh
	url = https://github.com/robbyrussell/oh-my-zsh.git
[submodule "shell/.config/shell/zsh/plugins/pure"]
	path = shell/.config/shell/zsh/plugins/pure
	url = https://github.com/sindresorhus/pure.git
[submodule "shell/.config/shell/zsh/themes/spaceship-prompt"]
	path = shell/.config/shell/zsh/themes/spaceship-prompt
	url = https://github.com/denysdovhan/spaceship-prompt.git
